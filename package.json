{
  "name": "angulartics2",
  "description": "Vendor-agnostic web analytics for Angular2 applications",
  "homepage": "https://angulartics.github.io/angulartics2/",
  "author": "Jo√£o Ribeiro <jonnybgod@gmail.com> (http://github.com/JonnyBGod)",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf dist",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "test": "ng test --watch=false --browsers=ChromeCI",
    "test:ci": "ng test --code-coverage --watch=false --no-progress --browsers=ChromeCI",
    "test:watch": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "presemantic-release": "cp -R .git dist/packages-dist",
    "semantic-release": "cd dist/packages-dist && semantic-release",
    "ghpages": "ng build --prod --no-progress --baseHref='/angulartics2/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.5",
    "@angular/cdk": "^7.3.2",
    "@angular/common": "^7.2.5",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "^7.2.5",
    "@angular/forms": "^7.2.5",
    "@angular/material": "^7.3.2",
    "@angular/platform-browser": "^7.2.5",
    "@angular/platform-browser-dynamic": "^7.2.5",
    "@angular/router": "^7.2.5",
    "@ng-bootstrap/ng-bootstrap": "4.0.3",
    "@uirouter/angular": "^3.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.2",
    "@angular/cli": "^7.3.2",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "^7.2.5",
    "@types/applicationinsights-js": "^1.0.9",
    "@types/facebook-pixel": "^0.0.19",
    "@types/fs-extra": "^5.0.5",
    "@types/google.analytics": "^0.0.39",
    "@types/jasmine": "^3.3.9",
    "@types/node": "~11.9.4",
    "@types/segment-analytics": "^0.0.30",
    "codelyzer": "^4.5.0",
    "fs-extra": "^7.0.1",
    "jasmine-core": "3.3.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "4.7.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "ts-node": "^8.0.2",
    "tsickle": "^0.34.3",
    "tslint": "^5.12.1",
    "typescript": "3.2.4"
  },
  "release": {
    "branch": "master"
  },
  "license": "MIT",
  "bugs": {
    "url": "http://github.com/angulartics/angulartics2/issues"
  },
  "repository": "angulartics/angulartics2",
  "engines": {
    "node": ">=8"
  }
}
