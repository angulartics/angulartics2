    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "3.0.0-rc.2",
    "protractor": "^5.3.1",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.2.0",
    "travis-deploy-once": "^5.0.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "2.7.x"
  },
  "release": {
    "branch": "master"
  },
  "license": "MIT",
  "bugs": {
    "url": "http://github.com/angulartics/angulartics2/issues"
  },
  "repository": "angulartics/angulartics2",
  "engines": {
    "node": ">=8"
  },
  "husky": {
    "hooks": {
